{
  "name": "{{ agave.appsPrefix }}-hello-world-gf",
  "version": "0.4",
  "label": "Updated hello world GeneFlow app demonstrating conditional execution",
  "defaultNodeCount": 1,
  "defaultMemoryPerNode": 1,
  "defaultProcessorsPerNode": 1,
  "executionType": "HPC",
  "parallelism": "SERIAL",
  "checkpointable": false,
  "executionSystem": "{{ agave.executionSystem }}",
  "deploymentPath": "{{ agave.appsDir }}/hello-world-gf-0.4",
  "deploymentSystem": "{{ agave.deploymentSystem }}",
  "templatePath": "hello-world-gf.sh",
  "testPath": "test/test.sh",
  "inputs": [
    { "id": "file",
      "value": {
        "required": true,
        "default": ""
      }
    }
  ],
  "parameters": [
    { "id": "mode",
      "value": {
        "required": false,
        "default": "basic",
        "type": "string"
      }
    },
    { "id": "output",
      "value": {
        "required": true,
        "type": "string"
      }
    },
    { "id": "exec_method",
      "value": {
        "required": true,
        "default": "auto",
        "type": "string"
      }
    }
  ]
}